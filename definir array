#include <iostream>
using namespace std;
#define N 10
int main() {
    //definition of an array
    int array[N]{9};
    //iterar array
    for (int i = 0; i < N; i++){
        array [i]= i+ i;
        cout << array[i]<<"\t";
    }
    return 0;

}
